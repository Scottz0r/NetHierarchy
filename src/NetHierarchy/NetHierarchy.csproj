<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;net452;net461</TargetFrameworks>
    <Version>2.0.1-alpha</Version>
    <Authors>Scott Clewell</Authors>
    <Company></Company>
    <Description>Hierarchy/unbalanced tree library. Allows for the management of parent/child hierarchies with the Node class. Implements LINQ like methods to traverse hierarchies.</Description>
    <PackageLicenseUrl>https://github.com/Scottz0r/NetHierarchy/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Scottz0r/NetHierarchy</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Scottz0r/NetHierarchy</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>hierarchy tree</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Copyright>Copyright 2017</Copyright>
    <PackageReleaseNotes>Netstandard 1.0 alpha release.</PackageReleaseNotes>
  </PropertyGroup>

</Project>